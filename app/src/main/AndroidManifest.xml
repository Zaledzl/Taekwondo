<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taekwondo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProgressActivity"></activity>
        <activity android:name=".DialogActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ToastActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".recyclerview.PuRecyclerViewActivity" />
        <activity android:name=".GridRecyclerViewActivity" />
        <activity android:name=".recyclerview.HorRecyclerViewActivity" />
        <activity
            android:name=".recyclerview.LinearRecyclerViewActivity"
            android:parentActivityName=".recyclerview.RecyclerViewActivity" />
        <activity
            android:name=".recyclerview.RecyclerViewActivity"
            android:parentActivityName=".UIActivity" />
        <activity
            android:name=".ZaleTestActivity"
            android:parentActivityName=".UIActivity" />
        <activity
            android:name=".ImageViewActivity"
            android:parentActivityName=".UIActivity" />
        <activity
            android:name=".EditTextActivity"
            android:parentActivityName=".UIActivity" />
        <activity
            android:name=".ButtonActivity"
            android:parentActivityName=".UIActivity" />
        <activity
            android:name=".TextViewActivity"
            android:parentActivityName=".UIActivity" />
        <activity
            android:name=".DisplayMessageActivity"
            android:parentActivityName=".UIActivity">

            <!-- The meta-data tag is required if you support API level 15 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UIActivity" />
        </activity>
        <activity android:name=".UIActivity" />
    </application>

</manifest>